{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "jquery-validation@1.19.1",
      "destination": "wwwroot/jquery-validation/",
      "files": [
        "dist/jquery.validate.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/jquery-validation-unobtrusive/",
      "files": [
        "dist/jquery.validate.unobtrusive.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery@3.4.1",
      "destination": "wwwroot/jquery/",
      "files": [
        "dist/jquery.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.4.1",
      "destination": "wwwroot/bootstrap/",
      "files": [
        "dist/css/bootstrap.css"
      ]
    }
  ]
}